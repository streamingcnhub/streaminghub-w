# Kopiuj do .env i wstaw swoje wartości. Nie commituj .env do repo.
PORT=3000
DB_PATH=./data/db.sqlite
CORS_ORIGIN=*

# Supabase (wstaw swoje wartości)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=eyJ...replace_with_your_anon_key...
# Service role key (Tylko dla backendu, NIE do frontendu)
SUPABASE_SERVICE_ROLE_KEY=eyJ...replace_with_your_service_role_key...

# Opcjonalnie JWT secret do przyszłego użycia
JWT_SECRET=replace_with_secret_for_future_use
